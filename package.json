{
  "name": "angular-static-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run angular-static-blog:serve-ssr",
    "serve:ssr": "node dist/angular-static-blog/server/main.js",
    "build:ssr": "ng build && ng run angular-static-blog:server",
    "prerender": "(ng serve> /dev/null 2>&1 &) && (ng run angular-static-blog:prerender --routes-file prerender-routes.txt) ; (fuser -k 4200/tcp)",
    "blog:render": "node blog-render.js",
    "blog:post": "node blog-post.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/platform-server": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@nguniversal/express-engine": "^13.0.1",
    "blob-polyfill": "^6.0.20211015",
    "cross-blob": "^3.0.0",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "simplelightbox": "github:ElectroluxV2/simplelightbox",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "asciidoctor": "^2.2.5",
    "asciidoctor-highlight.js": "^0.4.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "slugify": "^1.6.5",
    "typescript": "~4.5.2"
  }
}
